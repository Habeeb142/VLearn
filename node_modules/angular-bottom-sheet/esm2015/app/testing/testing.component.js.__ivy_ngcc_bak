import { Component, ViewChild } from '@angular/core';
import { AngularBottomSheetComponent } from '../component/angular-bottom-sheet.component';
let TestingComponent = class TestingComponent {
    constructor() { }
    ngOnInit() {
        this.options = {
            title: "Angular Bottom Sheet",
            enableCloseButton: true,
            darkTheme: false
        };
        this.open();
    }
    toggle() {
        this.testing.toggle();
    }
    open() {
        this.testing.open();
    }
    close() {
        this.testing.close();
    }
    toggleDarkTheme() {
        this.open();
        this.testing.toggleDarkTheme();
    }
    toggleCloseButton() {
        this.open();
        this.testing.toggleCloseButton();
    }
};
